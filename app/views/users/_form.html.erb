
<%= render 'shared/errors', obj: @user%>
<div class= "container mt-4">
<div class ="row justify-content-md-center">
<div class ="col-8">
  <%= form_with model: @user, local: true, data: { turbo: false } do |f| %>
  <p>
    <%= f.label :username %><br/>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :email %><br/>
    <%= f.email_field :email %>
  </p>
  <p>
  <%= f.label :password %><br/>
  <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
</div>
</div>
</div>